{
  "name": "react-event-booking",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "Syahrul Utomo",
  "license": "ISC",
  "description": "A simple clone version of Meetup.com",
  "scripts": {
    "start:staging": "cross-env process.env.NODE_ENV='staging' npm-run-all server:prod",
    "start:prod": "cross-env process.env.NODE_ENV='production' npm-run-all server:prod",
    "start:qa": "cross-env process.env.NODE_ENV='qa' npm-run-all server:dev",
    "start": "cross-env process.env.NODE_ENV='development' npm-run-all server:dev",
    "server:prod": "webpack-dev-server --hot --mode=production",
    "server:dev": "webpack-dev-server --hot --mode=development",
    "build:staging": "cross-env NODE_ENV='staging' && rm -rf dist && webpack --mode=production",
    "build:prod": "cross-env NODE_ENV='prod' && rm -rf dist && webpack --mode=production",
    "build:qa": "cross-env NODE_ENV='qa' && rm -rf dist && webpack --mode=development",
    "build": "cross-env NODE_ENV='dev' && rm -rf dist && webpack --mode=development",
    "prod": "webpack --config webpack.prod.js -p",
    "lint": "./node_modules/.bin/eslint source/",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "custom-env": "^2.0.1",
    "firebase": "^7.17.1",
    "moment": "^2.27.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/react": "^10.4.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.13.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
